<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thompsonc196">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThompsonC196">
        <activity
            android:name=".UI.TermAdd"
            android:exported="false" />
        <activity
            android:name=".UI.CourseAdd"
            android:exported="false" />
        <activity
            android:name=".UI.AssessAdd"
            android:exported="false" />
        <activity
            android:name=".UI.AssessDetail"
            android:exported="false"
            android:parentActivityName="UI.AssessList" />
        <activity
            android:name=".UI.CourseDetail"
            android:exported="false"
            android:parentActivityName="UI.CourseList" />
        <activity
            android:name=".UI.TermDetail"
            android:exported="false"
            android:parentActivityName="UI.TermList" />
        <activity
            android:name=".UI.AssessList"
            android:exported="false"
            android:parentActivityName=".UI.MainActivity" />
        <activity
            android:name=".UI.CourseList"
            android:exported="false"
            android:parentActivityName="UI.MainActivity" />
        <activity
            android:name=".UI.TermList"
            android:exported="false"
            android:parentActivityName="UI.MainActivity" />
        <activity
            android:name=".UI.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>